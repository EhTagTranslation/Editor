    "commander": "~12.1.0",
    "entities": "^4.5.0",
    "escape-string-regexp": "~5.0.0",
    "fastify": "^4.27.0",
    "fs-extra": "~11.2.0",
    "idb-keyval": "~6.2.1",
    "js-yaml": "~4.1.0",
    "lazysizes": "~5.3.2",
