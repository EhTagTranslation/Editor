{
  "name": "eh-tag-editor",
  "version": "3.0.0",
  "type": "module",
  "engines": {
    "node": ">= 16 <= 18",
    "yarn": "1.22"
  },
  "scripts": {
    "start:server": "nest start --debug --watch",
    "build:server": "nest build",
    "test:server": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config ./test/server/jest.config.js",
    "start:tool": "yarn build:tool --watch",
    "build:tool": "node scripts/tool.js",
    "test:tool": "yarn tool --help",
    "tool": "node --trace-deprecation --enable-source-maps -r dotenv/config ./dist/tool",
    "start:browser": "ng serve",
    "build:browser": "ng build --configuration production",
    "test:browser": "echo no test",
    "analyze:browser": "webpack-bundle-analyzer --mode=static --default-sizes=gzip --report=dist/browser/report.html dist/browser/stats.json",
    "clean": "rimraf dist",
    "format": "prettier --ignore-path .gitignore --write .",
    "lint": "eslint --ext .ts --ignore-path .gitignore --ignore-pattern **/*.spec.ts -- ."
  },
  "browser": {
    "./src/shared/ehentai/http/config.ts": "./src/shared/ehentai/http/config.browser.ts"
  },
  "imports": {
    "#*": "./dist/*.js"
  },
  "private": true,
  "dependencies": {
    "@actions/core": "~1.10.0",
    "@actions/exec": "~1.1.1",
    "@fastify/static": "^6.9.0",
    "@nestjs/common": "~9.3.9",
    "@nestjs/config": "~2.3.1",
    "@nestjs/core": "~9.3.9",
    "@nestjs/platform-fastify": "~9.3.9",
    "@nestjs/swagger": "~6.2.1",
    "@octokit/auth-app": "~4.0.9",
    "@octokit/auth-oauth-app": "~5.0.5",
    "@octokit/rest": "~19.0.7",
    "axios": "~1.3.4",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.0",
    "cli-color": "~2.0.3",
    "commander": "~10.0.0",
    "escape-string-regexp": "~5.0.0",
    "fs-extra": "~11.1.0",
    "idb-keyval": "~6.2.0",
    "js-yaml": "~4.1.0",
    "lazysizes": "~5.3.2",
    "linkify-it": "~4.0.1",
    "markdown-it": "~13.0.1",
    "node-cache": "~5.1.2",
    "pako": "~2.1.0",
    "parse5": "~7.1.2",
    "proxy-agent": "^5.0.0",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~7.8.0",
    "simple-git": "~3.16.1",
    "source-map-support": "~0.5.21",
    "tslib": "~2.5.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^15.0.0",
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular/animations": "^15.2.1",
    "@angular/cdk": "^15.2.1",
    "@angular/cli": "^15.2.1",
    "@angular/common": "^15.2.1",
    "@angular/compiler": "^15.2.1",
    "@angular/compiler-cli": "^15.2.1",
    "@angular/core": "^15.2.1",
    "@angular/forms": "^15.2.1",
    "@angular/language-service": "^15.2.1",
    "@angular/material": "^15.2.1",
    "@angular/platform-browser": "^15.2.1",
    "@angular/platform-browser-dynamic": "^15.2.1",
    "@angular/router": "^15.2.1",
    "@angular/service-worker": "^15.2.1",
    "@nestjs/cli": "~9.2.0",
    "@nestjs/schematics": "~9.0.4",
    "@nestjs/testing": "~9.3.9",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/cli-color": "~2.0.2",
    "@types/fs-extra": "~11.0.1",
    "@types/jest": "~29.4.0",
    "@types/js-yaml": "~4.0.5",
    "@types/markdown-it": "~12.2.3",
    "@types/node": "~18.14.5",
    "@types/pako": "~2.0.0",
    "@types/source-map-support": "~0.5.6",
    "@types/supertest": "~2.0.12",
    "@typescript-eslint/eslint-plugin": "~5.54.0",
    "@typescript-eslint/parser": "~5.54.0",
    "cross-env": "^7.0.3",
    "eslint": "~8.35.0",
    "eslint-config-prettier": "~8.6.0",
    "eslint-plugin-prettier": "~4.2.1",
    "jest": "~29.4.3",
    "jest-to-match-shape-of": "~1.3.2",
