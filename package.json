    "@nestjs/platform-fastify": "~8.2.6",
    "@nestjs/swagger": "~5.2.0",
    "@octokit/auth-app": "~3.6.1",
    "@octokit/auth-oauth-app": "~4.3.0",
    "@octokit/rest": "~18.12.0",
    "axios": "~0.25.0",
    "bluebird": "~3.7.2",
    "class-transformer": "~0.5.1",
