    "@angular/platform-browser-dynamic": "^21.0.2",
    "@angular/router": "^21.0.2",
    "@angular/service-worker": "^21.0.2",
    "@commander-js/extra-typings": "^14.0.0",
    "@eslint/js": "^9.39.1",
    "@nestjs/cli": "~11.0.14",
    "@nestjs/schematics": "~11.0.9",
