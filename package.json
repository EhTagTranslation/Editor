    "fastify-swagger": "~4.8.3",
    "fs-extra": "~10.0.0",
    "idb-keyval": "~5.0.6",
    "js-yaml": "~4.1.0",
    "lazysizes": "~5.3.2",
    "linkify-it": "~3.0.2",
    "markdown-it": "~12.1.0",
    "node-cache": "~5.1.2",
    "pako": "~2.0.3",
    "parse5": "~6.0.0",
