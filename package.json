    "fastify-swagger": "~4.14.0",
    "fs-extra": "~10.0.0",
    "idb-keyval": "~6.1.0",
    "js-yaml": "~4.1.0",
    "lazysizes": "~5.3.2",
    "linkify-it": "~3.0.3",
    "markdown-it": "~12.3.2",
    "node-cache": "~5.1.2",
    "pako": "~2.0.4",
    "parse5": "~6.0.0",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~7.5.3",
