{
  "name": "eh-tag-editor",
  "version": "2.0.0",
  "type": "module",
  "engines": {
    "node": "18",
    "yarn": "1.22"
  },
  "scripts": {
    "start:server": "nest start --debug --watch",
    "build:server": "nest build",
    "test:server": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config ./test/server/jest.config.js",
    "heroku-postbuild": "yarn build:server && yarn test:server && node scripts/server.js",
    "start:tool": "yarn build:tool --watch",
    "build:tool": "tsc --project ./src/tool",
    "test:tool": "yarn tool --help",
    "tool": "node ./dist/tool",
    "start:browser": "ng serve",
    "build:browser": "ng build --configuration production",
    "test:browser": "echo no test",
    "analyze:browser": "webpack-bundle-analyzer --mode=static --default-sizes=gzip --report=dist/browser/report.html dist/browser/stats.json",
    "clean": "rimraf dist",
    "format": "prettier --ignore-path .gitignore --write .",
    "lint": "eslint --ext .ts --ignore-path .gitignore --ignore-pattern **/*.spec.ts -- ."
  },
  "browser": {
    "./src/shared/ehentai/http/config.ts": "./src/shared/ehentai/http/config.browser.ts"
  },
  "imports": {
    "#*": "./dist/*.js"
  },
  "private": true,
  "dependencies": {
    "@actions/core": "~1.10.0",
    "@actions/exec": "~1.1.1",
    "@fastify/static": "^6.5.0",
    "@nestjs/common": "~9.1.6",
    "@nestjs/config": "~2.2.0",
    "@nestjs/core": "~9.1.6",
    "@nestjs/platform-fastify": "~9.1.6",
    "@nestjs/swagger": "~6.1.2",
    "@octokit/auth-app": "~4.0.7",
    "@octokit/auth-oauth-app": "~5.0.4",
    "@octokit/rest": "~19.0.5",
    "axios": "~1.1.3",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.13.2",
    "cli-color": "~2.0.3",
    "commander": "~9.4.1",
    "escape-string-regexp": "~5.0.0",
    "fs-extra": "~10.1.0",
    "idb-keyval": "~6.2.0",
    "js-yaml": "~4.1.0",
    "lazysizes": "~5.3.2",
    "linkify-it": "~4.0.1",
    "markdown-it": "~13.0.1",
    "node-cache": "~5.1.2",
    "pako": "~2.0.4",
    "parse5": "~7.1.1",
    "proxy-agent": "^5.0.0",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~7.5.7",
    "simple-git": "~3.14.1",
    "source-map-support": "~0.5.21",
    "tslib": "~2.4.0"
