    "jest": "~29.7.0",
    "jest-to-match-shape-of": "~1.3.2",
    "material-design-icons": "^3.0.1",
    "prettier": "~3.2.5",
    "rimraf": "~5.0.5",
    "supertest": "~6.3.4",
    "ts-jest": "~29.1.2",
