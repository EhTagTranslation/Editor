    "@typescript-eslint/eslint-plugin": "~5.41.0",
    "@typescript-eslint/parser": "~5.41.0",
    "cross-env": "^7.0.3",
    "eslint": "~8.26.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-prettier": "~4.2.1",
    "jest": "~29.2.2",
    "jest-to-match-shape-of": "~1.3.2",
