    "class-validator": "~0.14.3",
    "cli-color": "~2.0.4",
    "commander": "~14.0.2",
    "escape-string-regexp": "~5.0.0",
    "fastify": "^5.6.2",
    "fs-extra": "~11.3.2",
    "idb-keyval": "~6.2.2",
    "js-yaml": "~4.1.1",
    "lazysizes": "~5.3.2",
    "linkify-it": "~5.0.0",
    "markdown-it": "~14.1.0",
    "node-cache": "~5.1.2",
