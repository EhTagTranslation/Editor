{
  "name": "eh-tag-editor",
  "version": "2.0.0",
  "type": "module",
  "engines": {
    "node": "16",
    "yarn": "1.22"
  },
  "scripts": {
    "start:server": "nest start --debug --watch",
    "build:server": "nest build",
    "test:server": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config ./src/server/jest.config.js --coverage",
    "heroku-postbuild": "yarn build:server && yarn test:server && node scripts/server.js",
    "start:tool": "yarn build:tool --watch",
    "build:tool": "tsc --project ./src/tool",
    "test:tool": "yarn tool --help",
    "tool": "node ./dist/tool",
    "start:browser": "ng serve",
    "build:browser": "ng build --configuration production",
    "test:browser": "echo no test",
    "analyze:browser": "webpack-bundle-analyzer --mode=static --default-sizes=gzip --report=dist/browser/report.html dist/browser/stats.json",
    "clean": "rimraf dist",
    "format": "prettier --ignore-path .gitignore --write .",
    "lint": "eslint --ext .ts --ignore-path .gitignore --ignore-pattern **/*.spec.ts -- ."
  },
  "browser": {
    "./src/shared/ehentai/http/config.ts": "./src/shared/ehentai/http/config.browser.ts"
  },
  "imports": {
    "#*": "./dist/*"
  },
  "private": true,
  "dependencies": {
    "@actions/core": "~1.9.0",
    "@actions/exec": "~1.1.1",
    "@nestjs/common": "~8.4.7",
    "@nestjs/config": "~2.1.0",
    "@nestjs/core": "~8.4.7",
    "@nestjs/platform-fastify": "~8.4.7",
    "@nestjs/swagger": "~5.2.1",
    "@octokit/auth-app": "~3.6.1",
    "@octokit/auth-oauth-app": "~4.3.1",
    "@octokit/rest": "~18.12.0",
    "axios": "~0.27.2",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.13.2",
    "cli-color": "~2.0.2",
    "commander": "~9.3.0",
    "escape-string-regexp": "~5.0.0",
    "fastify-swagger": "~5.2.0",
    "fs-extra": "~10.1.0",
    "idb-keyval": "~6.1.0",
    "js-yaml": "~4.1.0",
    "lazysizes": "~5.3.2",
    "linkify-it": "~4.0.1",
    "markdown-it": "~13.0.1",
    "node-cache": "~5.1.2",
    "pako": "~2.0.4",
    "parse5": "~7.0.0",
    "proxy-agent": "^5.0.0",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~7.5.5",
    "simple-git": "~3.8.0",
    "source-map-support": "~0.5.21",
    "tslib": "~2.4.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.2",
    "@angular/animations": "^14.0.2",
    "@angular/cdk": "^14.0.2",
    "@angular/cli": "^14.0.2",
    "@angular/common": "^14.0.2",
    "@angular/compiler": "^14.0.2",
    "@angular/compiler-cli": "^14.0.2",
    "@angular/core": "^14.0.2",
    "@angular/forms": "^14.0.2",
    "@angular/language-service": "^14.0.2",
    "@angular/material": "^14.0.2",
    "@angular/platform-browser": "^14.0.2",
    "@angular/platform-browser-dynamic": "^14.0.2",
    "@angular/router": "^14.0.2",
    "@nestjs/cli": "~8.2.6",
    "@nestjs/schematics": "~8.0.11",
    "@nestjs/testing": "~8.4.7",
    "@types/cli-color": "~2.0.2",
    "@types/fs-extra": "~9.0.13",
    "@types/jest": "~28.1.2",
    "@types/js-yaml": "~4.0.5",
    "@types/markdown-it": "~12.2.3",
    "@types/node": "~18.0.0",
    "@types/pako": "~2.0.0",
    "@types/parse5": "~6.0.3",
    "@types/source-map-support": "~0.5.4",
    "@types/supertest": "~2.0.12",
    "@typescript-eslint/eslint-plugin": "~5.28.0",
    "@typescript-eslint/parser": "~5.28.0",
    "cross-env": "^7.0.3",
    "eslint": "~8.18.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-prettier": "~4.0.0",
    "jest": "~28.1.1",
    "jest-to-match-shape-of": "~1.3.2",
    "prettier": "~2.7.1",
    "rimraf": "~3.0.2",
    "supertest": "~6.2.3",
    "ts-jest": "~28.0.5",
    "tsconfig-paths": "~4.0.0",
    "type-fest": "~2.13.1",
    "typescript": "~4.7.4",
    "webpack-bundle-analyzer": "~4.5.0",
    "zone.js": "~0.11.6"
  }
}
