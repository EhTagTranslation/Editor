{
  "name": "eh-tag-editor",
  "version": "3.0.0",
  "type": "module",
  "engines": {
    "node": ">= 16 <= 18",
    "pnpm": "^8"
  },
  "scripts": {
    "start:server": "nest start --debug --watch",
    "build:server": "nest build",
    "test:server": "NODE_OPTIONS=\"${NODE_OPTIONS:-} --experimental-vm-modules\" jest --config ./test/server/jest.config.js",
    "start:tool": "pnpm build:tool --watch",
    "build:tool": "node scripts/tool.js",
    "test:tool": "pnpm tool --help",
    "tool": "node --trace-deprecation --enable-source-maps -r dotenv/config ./dist/tool",
    "start:browser": "ng serve",
    "build:browser": "ng build --configuration production",
    "test:browser": "echo no test",
    "analyze:browser": "webpack-bundle-analyzer --mode=static --default-sizes=gzip --report=dist/browser/report.html dist/browser/stats.json",
    "clean": "rimraf dist",
    "format": "prettier --ignore-path .gitignore --write .",
    "lint": "eslint --ext .ts --ignore-path .gitignore --ignore-pattern **/*.spec.ts -- ."
  },
  "browser": {
    "./src/shared/ehentai/http/config.ts": "./src/shared/ehentai/http/config.browser.ts"
  },
  "imports": {
    "#*": "./dist/*.js"
  },
  "private": true,
  "dependencies": {
    "@actions/core": "~1.10.0",
    "@actions/exec": "~1.1.1",
    "@actions/http-client": "~2.1.0",
    "@fastify/static": "^6.10.2",
    "@nestjs/common": "~10.0.5",
    "@nestjs/config": "~3.0.0",
    "@nestjs/core": "~10.0.5",
    "@nestjs/platform-fastify": "~10.0.5",
    "@nestjs/swagger": "~7.1.1",
    "@octokit/auth-app": "~6.0.0",
    "@octokit/auth-oauth-app": "~7.0.0",
    "@octokit/request-error": "^5.0.0",
    "@octokit/rest": "~19.0.13",
    "axios": "~1.4.0",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.0",
    "cli-color": "~2.0.3",
    "commander": "~11.0.0",
    "escape-string-regexp": "~5.0.0",
    "fastify": "^4.19.2",
    "fs-extra": "~11.1.1",
    "idb-keyval": "~6.2.1",
    "js-yaml": "~4.1.0",
    "lazysizes": "~5.3.2",
    "linkify-it": "~4.0.1",
    "markdown-it": "~13.0.1",
    "node-cache": "~5.1.2",
    "parse5": "~7.1.2",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~7.8.1",
    "simple-git": "~3.19.1",
    "source-map-support": "~0.5.21",
    "tslib": "~2.6.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.0",
    "@angular-devkit/build-angular": "^16.1.4",
    "@angular/animations": "^16.1.4",
    "@angular/cdk": "^16.1.4",
    "@angular/cli": "^16.1.4",
    "@angular/common": "^16.1.4",
    "@angular/compiler": "^16.1.4",
    "@angular/compiler-cli": "^16.1.4",
    "@angular/core": "^16.1.4",
    "@angular/forms": "^16.1.4",
    "@angular/language-service": "^16.1.4",
    "@angular/material": "^16.1.4",
    "@angular/platform-browser": "^16.1.4",
    "@angular/platform-browser-dynamic": "^16.1.4",
    "@angular/router": "^16.1.4",
    "@angular/service-worker": "^16.1.4",
    "@nestjs/cli": "~10.1.7",
    "@nestjs/schematics": "~10.0.1",
    "@nestjs/testing": "~10.0.5",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/cli-color": "~2.0.2",
    "@types/fs-extra": "~11.0.1",
    "@types/jest": "~29.5.2",
    "@types/js-yaml": "~4.0.5",
    "@types/markdown-it": "~12.2.3",
    "@types/node": "~20.4.1",
    "@types/source-map-support": "~0.5.6",
    "@types/supertest": "~2.0.12",
    "@typescript-eslint/eslint-plugin": "~5.61.0",
    "@typescript-eslint/parser": "~5.61.0",
    "dotenv": "^16.3.1",
    "eslint": "~8.44.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-prettier": "~4.2.1",
    "fflate": "^0.8.0",
